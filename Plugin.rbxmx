				<ProtectedString name="Source"><![CDATA[local SOUNDLOCALHOST = 4200
local ANIMATIONLOCALHOST = 6969

local HttpService = game:GetService("HttpService")
local MarketPlaceService = game:GetService("MarketplaceService")
local SoundService = game:GetService("SoundService")
local Signal = require(script.Signal)

local toolbar = plugin:CreateToolbar("kart's tools")
local pluginButton = toolbar:CreateButton("kart's tools", "Launch the python script first", "http://www.roblox.com/asset/?id=18239419915")
local notificationUi = script.NotificationUi
local gui = script.Frame
local notificationId = 0
local started = false
local busy = false

local widget = plugin:CreateDockWidgetPluginGui(
	"spoofer",
	DockWidgetPluginGuiInfo.new(
		Enum.InitialDockState.Float,
		false,
		false,
		300,
		200,
		300,
		200
	)
)

widget.Title = "sub to kartfr"
script.NotificationUi.Parent = game:GetService("CoreGui")
gui.Parent = widget

local function getNotificationId()
	notificationId += 1
	return notificationId
end

local function playSound(sound: Sound)
	local newSound = sound:Clone()
	newSound.Parent = widget
	newSound:Play()
	newSound.Ended:Wait()
	newSound:Destroy()
end

local function sendNotification(title: string, message: string, titleColor: Color3?)
	local notification = script.Notification:Clone()
	notification.Title.Text = title
	notification.Description.Text = message
	notification.Title.TextColor3 = titleColor or Color3.new(1, 0, 0)
	notification.LayoutOrder = getNotificationId()
	
	if titleColor == Color3.new(0, 1, 0) then
		task.spawn(playSound, script.Success)
	else
		local random = Random.new(os.clock() * 10000)
		if random:NextInteger(1, 10) == 10 then
			task.spawn(playSound, script.Boom) -- funny XD easter eg hah
		else
			task.spawn(playSound, script.Error)
		end
	end
	
	notification.Parent = notificationUi.Frame
	task.delay(2, notification.Destroy, notification)
end

local function try(...)
	local success, result
	for i=1, 3 do
		if success then break end
		if i > 1 then task.wait(1) end
		success, result = pcall(...)
	end
	return success, result
end

pluginButton.Click:Connect(function()
	widget.Enabled = not widget.Enabled
end)

gui.ImageLabel.AnimationButton.MouseButton1Down:Connect(function()
	if game.CreatorId == 0 then sendNotification("Error", "Publish game to group/profile.") return end
	if busy then sendNotification("Busy", "Plugin is busy.") return end
	local localHostStarted = pcall(HttpService.GetAsync, HttpService, `http://localhost:{ANIMATIONLOCALHOST}`)
	if not localHostStarted then sendNotification("Error", "Could not connect to local host.") return end
	
	busy = true
	sendNotification("Connected", "Successfully connected to localhost", Color3.new(0, 1, 0))
	
	local animationsToQuery = {}
	local animationInstances = {}
	local skippedAnimations = {}

	for i,v in game:GetDescendants() do
		if not v:IsA("Animation") then continue end
		local id = string.gsub(v.AnimationId, "%D", "")
		if #id <= 3 then table.insert(skippedAnimations, v) continue end
		if animationInstances[id] then table.insert(animationInstances[id], v) continue end

		animationInstances[id] = {}

		table.insert(animationInstances[id], v)
		table.insert(animationsToQuery, id)
	end
	
	local animationsBeingQueried = #animationsToQuery
	local animationsSucessfullyQueried = 0
	local completedSignal = Signal.new()
	local animationsToSend = {}

	task.spawn(function()
		for i, animationId in animationsToQuery do
			task.spawn(function()
				local success, result = try(MarketPlaceService.GetProductInfo, MarketPlaceService, animationId)
				if typeof(result) == "table" and result.AssetTypeId == 24 and result.Creator.Id ~= 1 and result.Creator.Id ~= game.CreatorId then -- 24 is animation assetid and 1 is roblox
					animationsToSend[animationId] = result.Name
					animationsSucessfullyQueried += 1 
					warn(`Looped through {animationsSucessfullyQueried} different animations.`)
				elseif typeof(result) ~= "table" then
					warn(`Failed getting product info from {animationId}. Error: {result}`)
					for i,v in animationInstances[animationId] do table.insert(skippedAnimations, v) end
				end 

				animationsBeingQueried -= 1
				if animationsBeingQueried == 0 then completedSignal:Fire() end
			end)

			task.wait(60/350) --throttle, in robloxs words: "Please don't issue more than 400 API requests/minute with server scripts and no more than 300 API requests/minute with local scripts."
		end
	end)
	
	completedSignal:Wait()
	sendNotification("Notification", `Sending {animationsSucessfullyQueried} animations to localhost`, Color3.new(0, 1, 0))
	
	if animationsSucessfullyQueried == 0 then
		warn(`All animations that are able to be sent are owned. Failed to send {#skippedAnimations} animation instances: `, table.unpack(skippedAnimations))
		busy = false
		return
	end
	
	warn(`Sending {animationsSucessfullyQueried} animations to localhost. Failed to send {#skippedAnimations} animation instances: `, table.unpack(skippedAnimations))
	
	local successfullySentAnimations = try(
		HttpService.PostAsync, 
		HttpService, 
		`http://localhost:{ANIMATIONLOCALHOST}`, 
		HttpService:JSONEncode({isGroup = game.CreatorType == Enum.CreatorType.Group and game.CreatorId, animations = animationsToSend})
	)

	if not successfullySentAnimations then 
		sendNotification("Stopped Uploading", "Failed to send animations to localhost.") 
		warn("Failed to send animations to localhost.")
		busy = false
		return 
	end
	
	while task.wait(1) do
		local success, animationsToChange = pcall(HttpService.GetAsync, HttpService, `http://localhost:{ANIMATIONLOCALHOST}`)
		if not success then sendNotification("Stopped Uploading", "Disconnected from localhost.") warn("Disconnected from localhost.") break end
		if animationsToChange == "done" then sendNotification("Notification", "Uploaded all animations.", Color3.new(0, 1, 0)) break end
		if animationsToChange == "" then continue end
		
		animationsToChange = HttpService:JSONDecode(animationsToChange)
		
		for oldId, newId in animationsToChange do
			for i, animation in animationInstances[oldId] do
				animation.AnimationId = "rbxassetid://" .. newId
			end
			
			warn(`Changed all animation ids: {oldId} ; {newId}`, "Instances:", table.unpack(animationInstances[oldId]))
		end
	end
	
	warn("Finished reuploading all animations.")
	busy = false
end)

gui.ImageLabel.GetAnimationButton.MouseButton1Down:Connect(function()
	if busy then sendNotification("Busy", "Plugin is busy.") return end
	warn("getting all unowned animations please be patient.")
	
	local animationsToQuery = {}
	local animationInstances = {}
	local unownedAnimations = {}
	local timeStarted = os.clock()

	for i,v in game:GetDescendants() do
		if not v:IsA("Animation") then continue end
		local id = string.gsub(v.AnimationId, "%D", "")
		if #id <= 3 then table.insert(unownedAnimations, v) continue end
		if animationInstances[id] then table.insert(animationInstances[id], v) continue end

		animationInstances[id] = {}

		table.insert(animationInstances[id], v)
		table.insert(animationsToQuery, id)
	end

	local animationsBeingQueried = #animationsToQuery
	local animationsSucessfullyQueried = 0
	local completedSignal = Signal.new()

	task.spawn(function()
		for i, animationId in animationsToQuery do
			task.spawn(function()
				local success, result = try(MarketPlaceService.GetProductInfo, MarketPlaceService, animationId)
				if typeof(result) ~= "table" then warn(`Failed getting product info from {animationId}.`) 
				elseif typeof(result) == "table" and result.AssetTypeId == 24 and result.Creator.Id ~= 1 and result.Creator.Id ~= game.CreatorId then -- 24 is animation assetid and 1 is roblox
					for i,v in animationInstances[animationId] do
						animationsSucessfullyQueried += 1
						table.insert(unownedAnimations, v)
					end
				end 

				animationsBeingQueried -= 1
				if animationsBeingQueried == 0 then completedSignal:Fire() end
			end)

			task.wait(60/350) --throttle, in robloxs words: "Please don't issue more than 400 API requests/minute with server scripts and no more than 300 API requests/minute with local scripts."
		end
	end)
	
	local reminderThread
	local function remind()
		warn(`still getting all unowned animations please be patient. Seconds passed: {math.floor(os.clock() - timeStarted)}. Number of animations Found: {animationsSucessfullyQueried}`)
		reminderThread = task.delay(30, remind)
	end
	
	reminderThread = task.delay(30, remind)
	completedSignal:Wait()
	warn(`{animationsSucessfullyQueried} unowned animation instances(click name to select in explorer): `, table.unpack(unownedAnimations))
	coroutine.close(reminderThread)
	busy = false
end)

gui.ImageLabel.SoundButton.MouseButton1Down:Connect(function()
	sendNotification("Not Availabe", "Coming soon...")
end)]]></ProtectedString>
